@using Microsoft.AspNetCore.Identity
@using ASP_Trello_Identity.Data 
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

<ul class="navbar-nav">
@if (!SignInManager.IsSignedIn(User))
{
    <li class="nav-item">
        <a class="nav-link" target="_self" asp-area="Identity" asp-page="/Account/Login">Войти</a>
    </li>
    <li class="nav-item">
        <a type="button" class="btn btn-success" target="_self" asp-area="Identity" asp-page="/Account/Register">Регистрация</a>
    </li>
}
</ul>
